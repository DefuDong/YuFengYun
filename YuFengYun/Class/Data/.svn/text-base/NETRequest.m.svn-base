#import "NETRequest.h"

@implementation NETRequest
- (NSMutableDictionary *)requestDic {
    if (!_requestDic) {
        _requestDic = [[NSMutableDictionary alloc] init];
    }
    return _requestDic;
}

- (id)init {
    self = [super init];
    if (self) {
        _requestDic = [[NSMutableDictionary alloc] init];
    }
    return self;
}

- (NSDictionary *)build {
    return nil;
}

- (NSString *)description {
    return [self.requestDic description];
}

@end
